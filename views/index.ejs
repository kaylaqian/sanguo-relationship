<!DOCTYPE html>
<html>
  <head>
    <title>AI 图分析引擎</title>
    <link rel="stylesheet" href="stylesheets/style.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  </head>

  <body>
    <div class="my-navbar">
      <div class="logo">
        <img src="images/u27.png">
      </div>
      <!-- <div class="title"> AI 图分析引擎</div> -->
    </div>
    <aside class="sidebar">
      <ul class="nav nav-tabs nav-tabs-left" name="algorithm">
        <li role="presentation"  class="active" value="show_graph">
          <a data-toggle="tab" href="#show_graph">
            <img src="images/u74.png">
          </a>
        </li>
        <li role="presentation" value="k-hop">
          <a data-toggle="tab" href="#k-hop">
            <img src="images/u76.png">
          </a>
        </li>
        <li role="presentation" value="path-search">
          <a data-toggle="tab" href="#path-search">
            <img src="images/u74.png">
          </a>
        </li>
        <li role="presentation" value="clus/ter">
          <a data-toggle="tab" href="#cluster">
            <img src="images/u76.png">
          </a>
        </li>
        <li role="presentation" value="property">
          <a data-toggle="tab" href="#property">
            <img src="images/u74.png">
          </a>
        </li>
      </ul>

      <div class="tab-content">

        <div role="tabpanel" class="tab-pane active" id="show_graph">
          <form>
            <div class="form-group">
              <label> 展示全图:</label>
              <select class="form-control">
                <option value="wiki graph">维基数据</option>
                <option value="sanguo">三国数据</option>
              </select>
            </div>
            <div class="form-group">
              <button type="submit" name='action' id='search' value='search' class="btn btn-primary pull-right">查询</button>
            </div>
          </form>
        </div>

        <div role="tabpanel" class="tab-pane" id="k-hop">
          <form>
            <div class="form-group algorithm">
              <label>Query ID</label>
              <input class="form-control" name="sourceId" placeholder="Source ID">
            </div>
            <div class="form-group">
              <label>K Value</label>
              <input class="form-control" name="kValue" placeholder="K 的值">
            </div>
            <div class="form-group">
              <button type="submit" name='action' id='k_hop_submit' value='k_hop_submit' class="btn btn-primary pull-right">查询</button>
            </div>
          </form>
        </div>

        <div role="tabpanel" class="tab-pane" id="path-search">
          <form>
            <div class="form-group algorithm">
              <label>Source ID</label>
              <input class="form-control" name="sourceId" placeholder="刘备">
            </div>
            <div class="form-group">
              <label>Target ID</label>
              <input class="form-control" name="targetId" placeholder="曹操">
            </div>
            <div class="form-group">
              <button type="submit" class="btn btn-primary pull-right">发现关系</button>
            </div>
          </form>
        </div>

        <div role="tabpanel" class="tab-pane" id="cluster">
          <form>
            <div class="form-group">
            <label> 势力集团</label>
            <button type="submit" class="btn btn-primary pull-right">查询</button>
            </div>
          </form>
        </div>

        <div role="tabpanel" class="tab-pane" id="property">
          <form class="form-horizontal">
            <div class="form-inline" id="leadership">
              <label class="col-xs-4"><input name="on" type="checkbox"> 统御</label>
              <div class="col-xs-10">
                <input class="form-control" name="expression" placeholder="表达式，例如 &lt; 50">
              </div>
            </div>

            <div class="form-inline" id="force">
              <label class="col-xs-4"><input name="on" type="checkbox"> 武力</label>
              <div class="col-xs-10">
                <input class="form-control" name="expression" placeholder="表达式，例如 &lt; 50">
              </div>
            </div>

            <div class="form-inline" id="intelligence">
              <label class="col-xs-4"><input name="on" type="checkbox"> 智力</label>
              <div class="col-xs-10">
                <input class="form-control" name="expression" placeholder="表达式，例如 &lt; 50">
              </div>
            </div>

            <div class="form-inline" id="politics">
              <label class="col-xs-4"><input name="on" type="checkbox"> 政治</label>
              <div class="col-xs-10">
                <input class="form-control" name="expression" placeholder="表达式，例如 &lt; 50">
              </div>
            </div>

            <div class="form-inline" id="charm">
              <label class="col-xs-4"><input name="on" type="checkbox"> 魅力</label>
              <div class="col-xs-10">
                <input class="form-control" name="expression" placeholder="表达式，例如 &lt; 50">
              </div>
            </div>
            
            <div class="form-inline" id="identity">
              <label class="col-xs-4"><input name="on" type="checkbox"> 身份</label>
              <div class="col-xs-10">
                <label><input type="radio" class="form-inline" name="identity" value="文臣"> 文臣</label>
                <label><input type="radio" class="form-inline" name="identity" value="武将"> 武将</label>
                <label><input type="radio" class="form-inline" name="identity" value="主公"> 主公</label>
              </div>
            </div>

            <div class="form-group">
              <button type="submit" class="btn btn-primary pull-right">查询</button>
            </div>
          </form>
        </div>
      </div>
    </aside>

    <section class="graph"></section>


    <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>

    <!-- jQuery Popup Overlay -->
    <script src="js/jquery.popupoverlay.js"></script>

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <!-- <script src="js/d3.v4.min.js"></script> 
    <script src="js/d3-force-cluster@lastest"></script>  -->
    <!-- <script src="http://d3js.org/queue.v1.min.js"></script>  -->
    <script src="http://d3js.org/d3.v2.js"></script> 

    <script src="js/app.js"></script>
    <script src="js/echarts.js"></script>  
    <script src="js/cluster-d3.js"></script>  
     <!-- <script src="js/cluster1.js"></script>       -->
     <script src="js/initGraph.js"></script>
    <script src="js/khopGraph.js"></script>
    <script src="js/pathGraph.js"></script>      
    <script src="js/propertyGraph.js"></script>    

  </body>
</html>
 